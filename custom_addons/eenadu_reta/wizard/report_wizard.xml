<odoo>
    <record id="wizard_eenadu_report_action" model="ir.actions.act_window">
        <field name="name">Eenadu Report</field>
        <field name="res_model">wizard.eenadu.report</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
    <record id="wizard_eenadu_report_from" model="ir.ui.view">
        <field name="name">Eenadu Report</field>
        <field name="model">wizard.eenadu.report</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="specific_date"/>
                    </group>
                </sheet>
                <footer>
                    <button name="print_report" string="Print Report" type="object"/>
                </footer>
            </form>
        </field>
    </record>

